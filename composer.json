{
    "name": "dkarlovi/xezilaires-dev",
    "type": "library",
    "description": "Iterate Excel spreadsheets, normalize rows into value objects, validate, serialize into CSV, JSON, XML",
    "license": "MIT",
    "authors": [
        {
            "name": "Dalibor KarloviÄ‡",
            "email": "dalibor.karlovic@flexolabs.io"
        }
    ],
    "require": {
        "php": ">= 7.2.0",
        "ext-json": "*",
        "symfony/options-resolver": "^3.4 || ^4.0 || ^5.0",
        "symfony/serializer": "^3.4 || ^4.0 || ^5.0"
    },
    "replace": {
        "dkarlovi/xezilaires-core": "self.version",
        "dkarlovi/xezilaires-phpspreadsheet": "self.version",
        "dkarlovi/xezilaires-spout": "self.version",
        "dkarlovi/xezilaires-symfony": "self.version"
    },
    "conflict": {
        "box/spout": "< 3.0",
        "phpoffice/phpspreadsheet": "< 1.4.1"
    },
    "require-dev": {
        "box/spout": "^3.0",
        "doctrine/annotations": "^1.11",
        "matthiasnoback/symfony-dependency-injection-test": "^4.0",
        "nicmart/tree": "^0.2,>= 0.2.4",
        "nyholm/nsa": "^1.1",
        "nyholm/symfony-bundle-test": "^1.5",
        "phpoffice/phpspreadsheet": "^1.4,>= 1.4.1",
        "phpunit/phpunit": "^8.5",
        "symfony/console": "^3.4 || ^4.0 || ^5.0",
        "symfony/framework-bundle": "^3.4 || ^4.0 || ^5.0",
        "symfony/property-access": "^3.4 || ^4.0 || ^5.0",
        "symfony/property-info": "^3.4 || ^4.0 || ^5.0",
        "symfony/validator": "^3.4 || ^4.0 || ^5.0"
    },
    "suggest": {
        "box/spout": "Read Excel files with Spout",
        "phpoffice/phpspreadsheet": "Read Excel files with PhpSpreadsheet",
        "symfony/console": "bin/xezilaires use",
        "symfony/framework-bundle": "Symfony bundle integration, bin/xezilaires use",
        "symfony/validator": "Symfony bundle integration, bin/xezilaires validate use"
    },
    "config": {
        "platform": {
            "ext-gd": "1.3.1"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Xezilaires\\": "src/Xezilaires",
            "Xezilaires\\Bridge\\": "src/Bridge"
        },
        "exclude-from-classmap": [
            "**/src/Xezilaires/Test/",
            "**/src/Bridge/*/Test/"
        ]
    },
    "bin": [
        "src/Bridge/Symfony/bin/xezilaires"
    ]
}
